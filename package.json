{
  "name": "@buttercup/iconographer",
  "version": "0.5.0",
  "description": "Icon queuing, fetching and caching library",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel -d dist source",
    "docs": "jsdoc2md source/**/*.js > API.md",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "prepublishOnly": "run-s test build",
    "pub": "np",
    "test": "run-s build test:unit test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:unit": "mocha -r test/index.js 'test/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup/iconographer.git"
  },
  "keywords": [
    "icon",
    "store",
    "download",
    "image",
    "entry",
    "buttercup"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup/iconographer/issues"
  },
  "homepage": "https://github.com/buttercup/iconographer#readme",
  "dependencies": {
    "@buttercup/channel-queue": "~0.2.2",
    "eventemitter3": "~3.0.0",
    "icojs": "~0.10.1",
    "is-node": "~1.0.2",
    "node-fetch": "~2.0.0",
    "parse5": "~3.0.3",
    "url": "~0.11.0"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "~0.14.3",
    "jsdoc-to-markdown": "~4.0.1",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "np": "~2.19.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "prettier-check": "^2.0.0",
    "sinon": "^4.2.2",
    "sleep-promise": "~2.0.0",
    "temp-dir": "~1.0.0"
  }
}
