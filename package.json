{
  "name": "@buttercup/iconographer",
  "version": "1.1.0",
  "description": "Icon queuing, fetching and caching library",
  "main": "source/index.js",
  "files": [
    "resources/**/*",
    "source/**/*",
    "API.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "node ./prepare/index.js",
    "build:missing": "node ./prepare/index.js --process=missing",
    "docs": "jsdoc2md source/**/*.js > API.md",
    "format": "prettier --write '{source,test}/**/*.js'",
    "precommit": "lint-staged",
    "test": "run-s test:unit test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:unit": "mocha -r test/index.js 'test/**/*.spec.js'",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup/iconographer.git"
  },
  "keywords": [
    "icon",
    "store",
    "download",
    "image",
    "entry",
    "buttercup"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup/iconographer/issues"
  },
  "homepage": "https://github.com/buttercup/iconographer#readme",
  "dependencies": {
    "strong-data-uri": "^1.0.6"
  },
  "devDependencies": {
    "arraybuffer-to-buffer": "0.0.5",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "execa": "^1.0.0",
    "file-type": "^7.5.0",
    "get-website-favicon": "0.0.7",
    "gif-frames": "^1.0.1",
    "husky": "~0.14.3",
    "icojs": "^0.12.3",
    "image-size": "^0.7.1",
    "jimp": "^0.6.0",
    "jsdoc-to-markdown": "~4.0.1",
    "lint-staged": "^6.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.0",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "p-all": "^1.0.0",
    "parse5": "^3.0.3",
    "prettier": "^1.10.2",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.3",
    "sinon": "^4.2.2",
    "stream-to-buffer": "^0.1.0",
    "svg2png": "^4.1.1",
    "url": "^0.11.0"
  }
}
